{
  "name": "Project Dashboard",
  "version": "1.0.0",
  "description": "Tableau de bord des métriques projet pour Trimble Connect - Visualisez vos Notes, BCF, Fichiers et Vues en temps réel",
  "author": "Votre Entreprise",
  "api": "1.0",
  "extensions": [
    {
      "type": "panel",
      "id": "dashboard-panel",
      "title": "Dashboard",
      "icon": "icon.png",
      "entryPoint": "../dist/index.js"
    }
  ],
  "permissions": [
    "project.read",
    "notes.read",
    "bcf.read",
    "files.read",
    "views.read"
  ],
  "dependencies": {
    "@trimble/connect-workspace-api": "^2.0.0"
  },
  "license": "MIT",
  "homepage": "https://votre-site.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/votre-repo/trimble-dashboard"
  },
  "keywords": [
    "trimble",
    "connect",
    "dashboard",
    "metrics",
    "bim",
    "bcf"
  ]
}
