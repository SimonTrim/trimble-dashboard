{
  "name": "Project Dashboard",
  "version": "1.0.0",
  "description": "Tableau de bord des metriques projet pour Trimble Connect - Visualisez vos Notes, BCF, Fichiers et Vues en temps reel",
  "author": "Trimble",
  "api": "1.0",
  "extensions": [
    {
      "type": "panel",
      "id": "dashboard-panel",
      "title": "Dashboard",
      "icon": "https://simontrim.github.io/trimble-dashboard/public/icon-48.png",
      "entryPoint": "https://simontrim.github.io/trimble-dashboard/dist/index.js"
    }
  ],
  "permissions": [
    "project.read",
    "notes.read",
    "bcf.read",
    "files.read",
    "views.read"
  ],
  "dependencies": {
    "@trimble/connect-workspace-api": "^2.0.0"
  }
}
