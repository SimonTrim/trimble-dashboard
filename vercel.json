{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/api/index.js"
    }
  ],
  "functions": {
    "backend/api/index.js": {
      "maxDuration": 10,
      "memory": 1024
    }
  }
}
